{
     "version": 2,
     "builds": [
       {
         "src": "src/index.js",
         "use": "@vercel/node"
       }
     ],
     "routes": [
       { "src": "/(.*)", "dest": "/src/$1" },
       { "handle": "filesystem" },
       { "src": "/(.*)", "status": 404, "dest": "404.html" }
     ]
   }
   